{% macro Container(
  class,
  small,
  fullWidth = false,
  padding = true,
  attributes
) %}
  <div
    {{ attributes }}
    class="Container {{ class }}"
    data-container-padding="{{ padding }}"
    data-container-fullwidth="{{ fullWidth }}"
    data-container-small="{{ small }}""
  >
    {{ caller() }}
  </div>
{% endmacro %}
